{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "es2020"
        ],
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "out",
        "plugins": [
            {
                "name": "typescript-tslint-plugin"
            }
        ],
        "rootDir": "src",
        "sourceMap": true,
        "strict": true,
        "target": "es2020"
    }
}
